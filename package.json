{
    "name": "obsidian-date-selector",
    "version": "1.0.0",
    "description": "A plugin for Obsidian that provides a date selector triggered by @ symbol or clicking dates",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json"
    },
    "keywords": [
        "obsidian",
        "plugin",
        "date-selector"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^22.14.1",
        "@typescript-eslint/eslint-plugin": "^5.2.0",
        "@typescript-eslint/parser": "^5.2.0",
        "builtin-modules": "^3.2.0",
        "esbuild": "^0.25.2",
        "obsidian": "latest",
        "tslib": "2.3.1",
        "typescript": "^5.8.3"
    }
}
